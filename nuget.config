<?xml version="1.0" encoding="utf-8"?>
<configuration>

  <packageSources>
    <!--To inherit the global NuGet package sources remove the <clear/> line below -->
    <clear />
    <add key="nuget" value="https://api.nuget.org/v3/index.json" />
    <add key="nc-dotnet-source" value="https://source.netcompany.com/tfs/Foundations/_packaging/dotnet/nuget/v3/index.json" />
  </packageSources>

  <packageSourceMapping>
    <packageSource key="nuget">
      <package pattern="*" />
    </packageSource>
    <packageSource key="nc-dotnet-source">
      <package pattern="Netcompany.Net.*" />
    </packageSource>
  </packageSourceMapping>

</configuration>
